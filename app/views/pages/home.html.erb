<h1>DATABASE INFO</h1>
<h2>Users</h2>
Name   Email<br>
<% @users.each do |user| %>
	<%= user.username %>
	<%= user.email %>
</br>
<% end %>
<h2>Cats</h2>
<% @cats.each do |cat| %>
	<%= cat.name %>
</br>
<% end %>
<h2>To-Dos</h2>
<% @todos.each do |todo| %>
	<% if !todo.is_complete %>
		<%= todo.task %>
	<%end%>
<% end %>